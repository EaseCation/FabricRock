{
  "schemaVersion": 1,
  "id": "bedrock-loader",
  "version": "${version}",
  "name": "Bedrock Loader",
  "description": "Effortlessly transforming Minecraft Bedrock Addons into new horizons for Fabric.",
  "authors": [
    "Choi-Jungwoo"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/bedrock-loader/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "net.easecation.bedrockloader.BedrockLoader",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "net.easecation.bedrockloader.BedrockLoaderDataGenerator",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "bedrock-loader.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.6",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.22"
  },
  "suggests": {
    "another-mod": "*"
  }
}